<?xml version="1.0"?>

<PropertyList>

  <path>Models/icon-a5.ac</path>
  <texture-path>Models/textures</texture-path>

  <model>
    <name>cockpit</name>
    <path>Aircraft/icon-a5/Models/cockpit.xml</path>
  </model>

  <!-- SHADOW -->

  <animation>
    <type>rotate</type>
    <object-name>shadow</object-name>
    <property>orientation/roll-deg</property>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>shadow</object-name>
    <property>orientation/pitch-deg</property>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>shadow</object-name>
    <property>position/altitude-agl-ft</property>
    <factor>0.25</factor>
    <offset-m>-1.25</offset-m>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>shadow</object-name>
    <property>gear/gear[8]/compression-m</property>
    <factor>-1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>shadow</object-name>
    <property>gear/gear[2]/wow</property>
    <factor>-0.1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>shadow</object-name>
    <property>orientation/roll-deg</property>
    <condition>
      <and>
	<property>gear/gear[8]/wow</property>
	<property>gear/gear[9]/wow</property>
	<property>gear/gear[10]/wow</property>
      </and>
    </condition>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>shadow</object-name>
    <property>orientation/pitch-deg</property>
    <condition>
      <and>
	<property>gear/gear[8]/wow</property>
	<property>gear/gear[9]/wow</property>
	<property>gear/gear[10]/wow</property>
      </and>
    </condition>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <!-- SPRAY -->

